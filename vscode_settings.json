{
  "editor.fontLigatures": true, //"editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'liga'",
  "editor.fontFamily": "Cascadia Code PL",
  "workbench.colorTheme": "One Dark Pro Mix",
  "oneDarkPro.editorTheme": "One Dark Pro Darker",
  "oneDarkPro.bold": true,
  "oneDarkPro.vivid": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "workbench.editor.showTabs": "multiple", //none
  "window.menuBarVisibility": "toggle",
  "editor.minimap.enabled": false, //true
  "editor.scrollbar.vertical": "hidden",
  "editor.overviewRulerBorder": false, //true
  "editor.hideCursorInOverviewRuler": true, //false
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.glyphMargin": false, //true
  "indenticator.width": 1,
  "indenticator.color.dark": "rgba(255,255,255,0.1)",
  "workbench.iconTheme": "material-icon-theme",
  "editor.matchBrackets": "never",
  "workbench.statusBar.visible": false, //true
  "editor.cursorBlinking": "expand",
  "editor.linkedEditing": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "breadcrumbs.filePath": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "security.workspace.trust.untrustedFiles": "open",
  // "workbench.editor.customLabels.patterns": {
  //   "**/types/**": "${filename}.${extname} - Types",
  //   "**/pages/**": "${filename}.${extname} - Pages",
  //   "**/components/**": "${filename}.${extname} - Components",
  //   "**/data/**": "${filename}.${extname} - Data",
  //   "**/assets/**": "${filename}.${extname} - Assets"
  // },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.mouseWheelZoom": false,
  "workbench.startupEditor": "none",
  "editor.stickyScroll.enabled": false,
  "workbench.sideBar.location": "right"
}
